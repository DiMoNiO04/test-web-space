{
  "parser": "@babel/eslint-parser",
  "extends": ["next/core-web-vitals", "eslint:recommended", "plugin:react/recommended"],
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "globals": {
    "React": "writable"
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-undef": "off"
  }
}
